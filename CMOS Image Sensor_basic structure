implant ars dose=2e14 energy=30 tilt=7 pearson

diffuse time=5 temp=900 press=1.0




#
etch barrier all
etch silicon         start x=0  y=-1
etch                 cont  x=0  y=0
etch                 cont  x=1.4 y=0
etch                 done  x=1.4 y=-1

deposit silicon thick=0.05 divisions=3

etch silicon         start x=0  y=-1
etch                 cont  x=0  y=0
etch                 cont  x=1.4 y=0
etch                 done  x=1.4 y=-1



#TG
deposit oxide thick=0.006 divisions=3
deposit polysilicon thickness=0.1 divisions=10 dy=0.005 ydy=0.1
deposit polysilicon thickness=0.1 divisions=10 dy=0.005 ydy=0  
etch polysilicon left p1.x=0.8
etch polysilicon right p1.x=1.1
deposit oxide thick=0.006 divisions=3


struct outfile=cis_basic.str

go atlas


#electrode
elec num=1 name=TG x.min=0.8 x.max=1.1 y.min=0 y.max=0
elec num=2 name=FD x.min=1.1 x.max=1.35 y.min=0 y.max=0
elec num=3 name=Body x.min=0 x.max=1.4 y.min=1.6 y.max=1.6
contact num=1 neutral
contact num=2 neutral
contact num=3 neutral


models conmob srh auger bgn fldmob cvt fermi


method newton trap autonr
output con.band val.band

probe name=elec_pd x=0.2 y=0.85 n.conc
probe name=elec_fd x=1.2 y=0.1 n.conc

solve init
solve prev


solve vdrain=3.3 name=FD
save outf=cis_basic_1.str
log outf=cis_basic_1.log master

solve vgate=0 name=TG
save outf=basic_carrier_off.str

solve vgate=3.3 name=TG
save outf=basic_carrier_on.str

solve vgate=0 vstep=0.1 vfinal=3.3 name=TG
log outf=basic_vtg_sweep.log
save outf=2.str


solve vgate=7 vstep=-0.1 vfinal=0 name=TG
log outf=3.log
save outf=3.str


solve vgate=0.0 vstep=0.1 vfinal=0.5 name=TG
log outf=4.log
save outf=4.str


solve vgate=0.5 vstep=0.1 vfinal=1.0 name=TG
log outf=5.log
save outf=5.str


solve vgate=1.0 vstep=0.1 vfinal=1.5 name=TG
log outf=6.log
save outf=6.str


solve vgate=1.5 vstep=0.1 vfinal=2.0 name=TG
log outf=7.log
save outf=7.str


solve vgate=2.0 vstep=0.1 vfinal=2.5 name=TG
log outf=8.log
save outf=8.str


solve vgate=2.5 vstep=0.1 vfinal=3.0 name=TG
log outf=9.log
save outf=9.str

quit
