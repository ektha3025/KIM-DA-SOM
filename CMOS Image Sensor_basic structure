go athena

line x location=0  spacing=0.05
line x location=0.06  spacing=0.01
line x location=0.11  spacing=0.01
line x location=0.13  spacing=0.01
line x location=0.2  spacing=0.01
line x location=0.23  spacing=0.01
line x location=0.3  spacing=0.01
line x location=0.5  spacing=0.01
line x location=0.6  spacing=0.01
line x location=0.7  spacing=0.01
line x location=0.78  spacing=0.01
line x location=0.8  spacing=0.01
line x location=0.83  spacing=0.01
line x location=0.85  spacing=0.01
line x location=1  spacing=0.03
line x location=1.15  spacing=0.03
line x location=1.35   spacing=0.03
line x location=1.4 spacing=0.1

line y location=0 spacing=0.1
line y location=0.4 spacing=0.1
line y location=0.6 spacing=0.1
line y location=0.8 spacing=0.1
line y location=1.0 spacing=0.1
line y location=1.2 spacing=0.1
line y location=1.4 spacing=0.1
line y location=1.6 spacing=0.2



init silicon c.boron=1.0e16 orientation=100

method full.cpl

epitaxy time=1 temp=1050 thickness=1 divisions=8 dy=0.1 ydy=1 \
        c.boron=1e14
epitaxy time=1 temp=1050 thickness=1 divisions=16 dy=0.002 ydy=0 \
        c.boron=1e14


etch silicon  start x=0 y=-1
etch                 cont  x=0 y=0
etch                 cont  x=2 y=0
etch                 done  x=2 y=-1


#p-well
deposit barrier thick=0.03 divisions=3
etch barrier  start x=0.06 y=-1
etch                 cont  x=0.06 y=0
etch                 cont  x=0.1 y=0
etch                 done  x=0.1 y=-1
etch barrier right p1.x=1.75
#implant boron energy=150 dose=2e13 tilt=0 rotation=0 bca n.ion=200
#implant boron energy=150 dose=2.4e13 tilt=0 rotation=0 bca n.ion=150
implant boron energy=170  dose=1.2e13 tilt=0 rotation=0 bca n.ion=200
etch barrier all


method full.cpl grid.ox=0.003


#STI
deposit oxide thick=0.006 divisions=3
diffus time=1 temperature=1050 nitro

deposit material=photoresist thick=0.5 divisions=5

etch photoresist start x=0.01 y=-3
etch        cont  x=0.01  y=3
etch    cont x=0.06 y=3
etch       done x=0.06 y=-3

etch photoresist start x=1.35 y=-3
etch        cont  x=1.35  y=3
etch    cont x=1.4 y=3
etch       done x=1.4 y=-3


etch oxide start x=0.01 y=-0.006
etch        cont  x=0.01  y=0
etch    cont x=0.06 y=0
etch       done x=0.06 y=-0.006

etch oxide start x=1.35 y=-0.006
etch        cont  x=1.35  y=0
etch    cont x=1.4 y=0
etch       done x=1.4 y=-0.006


etch silicon start x=0.01 y=0
etch        cont  x=0.01  y=0.3
etch    cont x=0.06 y=0.3
etch       done x=0.06 y=0

etch silicon start x=1.35 y=0
etch        cont  x=1.35  y=0.3
etch    cont x=1.4 y=0.3
etch       done x=1.4 y=0


deposit oxide thick=1

etch material=oxide  start x=0 y=-1
etch                 cont  x=0 y=0
etch                 cont  x=2 y=0
etch                 done  x=2 y=-1



#PD
etch silicon         start x=0.06  y=-1
etch                 cont  x=0.06  y=0.8
etch                 cont  x=0.11 y=0.8
etch                 done  x=0.11 y=-1

etch material=silicon  start x=0.8 y=-1
etch                 cont  x=0.8 y=0.8
etch                 cont  x=0.85 y=0.8
etch                 done  x=0.85 y=-1

etch silicon         start x=0  y=-1
etch                 cont  x=0 y=0
etch                 cont  x=1.4 y=0
etch                 done  x=1.4 y=-1

deposit barrier thick=0.01 divisions=3

etch barrier  start x=0.11 y=-1
etch          cont  x=0.11 y=1
etch          cont  x=0.8 y=1
etch          done  x=0.8 y=-1

implant ars dose=5e12 energy=90 tilt=7 pearson
implant ars dose=4e12 energy=90 tilt=7 pearson
implant ars dose=3e12 energy=90 tilt=7 pearson
